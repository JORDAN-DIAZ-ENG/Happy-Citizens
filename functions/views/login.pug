
extends layout.pug 

block content 
    div.text-center 
        form(method="POST", action="/submit")
            label(for = "username") Username:
            input(type = "text", name = "username")
            br
            label(for = "password") Password:
            input(type = "password", name = "password")
            br
            input(type = "submit", name = "submit", value = "submit")
